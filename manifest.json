{
    "theme_color": "#252c3c",
    "background_color": "#252c3c",
    "display": "standalone",
    "scope": ".",
    "start_url": "/",
    "name": "AI Sensei",
    "short_name": "AI Sensei",
    "description": "",
    "icons": [{
        "src": "/img/pwa-icon-192x192.png",
        "sizes": "192x192",
        "type": "image/png",
        "purpose": "any"
    }, {
        "src": "/img/pwa-icon-maskable-192x192.png",
        "sizes": "192x192",
        "type": "image/png",
        "purpose": "maskable"
    }, {
        "src": "/img/pwa-icon-384x384.png",
        "sizes": "384x384",
        "type": "image/png",
        "purpose": "any"
    }, {
        "src": "/img/pwa-icon-maskable-384x384.png",
        "sizes": "384x384",
        "type": "image/png",
        "purpose": "maskable"
    }, {
        "src": "/img/pwa-icon-512x512.png",
        "sizes": "512x512",
        "type": "image/png",
        "purpose": "any"
    }, {
        "src": "/img/pwa-icon-maskable-512x512.png",
        "sizes": "512x512",
        "type": "image/png",
        "purpose": "maskable"
    }],
    "file_handlers": [{
        "action": "/upload",
        "accept": {
            "application/x-go-sgf": [".sgf"]
        },
        "icons": [{
            "src": "/img/pwa-icon-192x192.png",
            "sizes": "192x192",
            "type": "image/png"
        }],
        "launch_type": "single-client"
    }],
    "share_target": {
        "action": "/pwa-share-data",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "files": [
                {
                "name": "sgf",
                "accept": ["application/x-go-sgf", ".sgf"]
            }]
        }
    }
}
